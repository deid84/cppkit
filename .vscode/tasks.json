{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Clean Build Lifecycle",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"linux": {
				"command": "./clean.py"
			},
			"windows": {
				"command": "python clean.py"
			},
			"presentation": {
				"reveal": "always",
				"panel": "dedicated",
				"focus": true,
				"clear": true
			},
			"problemMatcher": []
		}
	]
}